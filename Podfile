# Uncomment the next line to define a global platform for your project
 platform :ios, '7.1'

target 'blerter-mobile' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
   
  # Pods for BlerterMobile
  pod 'BlerterService', :path => '.'

  target 'blerter-mobileTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'blerter-mobileUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
